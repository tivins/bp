{
  "name": "@tivins/bp",
  "version": "1.0.4",
  "description": "Awesome blueprints and components for the web.",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test",
    "build": "tsc && npm run css",
    "dev_ts": "tsc -w",
    "http": "node examples/http.js",
    "css": "cleancss -o dist/theme.css theme/main.css"
  },
  "keywords": [],
  "author": "Tivins",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.8",
    "clean-css-cli": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  }
}
